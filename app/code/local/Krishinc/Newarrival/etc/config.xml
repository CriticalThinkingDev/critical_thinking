<?xml version="1.0"?>
<config>
    <modules>
        <Krishinc_Newarrival>
            <version>0.1.0</version>
        </Krishinc_Newarrival>
    </modules>
    <frontend>
        <routers>
            <newarrival>
                <use>standard</use>
                <args>
                    <module>Krishinc_Newarrival</module>
                    <frontName>new</frontName>
                </args>
            </newarrival>
        </routers>
        <layout>
            <updates>
                <newarrival>
                    <file>newarrival.xml</file>
                </newarrival>
            </updates>
        </layout>
    </frontend>
    <global>
        <blocks>
            <newarrival>
                <class>Krishinc_Newarrival_Block</class>
            </newarrival>
        </blocks>
        <helpers>
            <newarrival>
                <class>Krishinc_Newarrival_Helper</class>
            </newarrival>
        </helpers>
    </global>
</config>